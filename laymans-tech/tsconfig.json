{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "shared-component": [
        "dist/shared-component"
      ],
      "shared-component/*": [
        "dist/shared-component/*"
      ],
      "core": [
        "dist/core"
      ],
      "core/*": [
        "dist/core/*"
      ],
      "portfolio": [
        "dist/portfolio/portfolio",
        "dist/portfolio"
      ]
    }
  }
}